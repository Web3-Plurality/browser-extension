{
    "version": "1.0.0",
    "manifest_version": 3,
    "name": "Plurality",
    "description": "Identity extension with semaphore",
    "action": {
        "default_popup": "js/index.html",
        "default_title": "Plurality"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["js/contentScript.js"]
        }
      ],
      "permissions": [
        "activeTab",
        "storage",
        "scripting"
        ]
}